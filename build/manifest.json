{
    "name": "SyphonX",
    "version": "1.0.0",
    "manifest_version": 3,
    "minimum_chrome_version": "10.0",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "devtools_page": "devtools.html",
    "action": {}
  }
