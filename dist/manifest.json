{
  "name": "SyphonX",
  "version": "0.9.4",
  "manifest_version": 3,
  "minimum_chrome_version": "10.0",
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "webRequest"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "devtools_page": "devtools.html",
  "action": {}
}
